        "def solution(lst, target):\n",
        "    lookup = {}\n",
        "    for i, num in enumerate(lst):\n",
        "      if target - num in lookup:\n",
        "        return(lookup[target - num], lst[i])\n",
        "      lookup[num] = lst[i]\n",
        "print(solution([10,20,10,40,60,70], 50))"
      ]
    }
